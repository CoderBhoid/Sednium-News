{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "Sednium-News/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/proxy",
            "dest": "/api/proxy.js"
        },
        {
            "src": "/api/news",
            "dest": "/api/news.js"
        },
        {
            "src": "/api/read",
            "dest": "/api/read.js"
        },
        {
            "src": "/rss/(.*)",
            "dest": "/api/rss.js?category=$1"
        },
        {
            "src": "/rss",
            "dest": "/api/rss.js"
        },
        {
            "src": "/(.*)",
            "dest": "/Sednium-News/$1"
        }
    ]
}