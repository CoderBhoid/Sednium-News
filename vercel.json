{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "N-News/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/proxy",
            "dest": "/api/proxy.js"
        },
        {
            "src": "/rss",
            "dest": "/api/rss.js"
        },
        {
            "src": "/(.*)",
            "dest": "/N-News/$1"
        }
    ]
}